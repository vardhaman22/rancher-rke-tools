{
  "extends": [
    "github>rancher/renovate-config#release"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "(^|/|\\.)Dockerfile$",
        "(^|/)Dockerfile[^/]*$"
      ],
      "matchStrings": [
        "ENV DOCKER_VERSION(\\=|\\s)(?<currentValue>.*?)\\n"
      ],
      "depNameTemplate": "moby/moby",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^v?(?<version>\\d+\\.\\d+\\.\\d+)$"
    }
  ],
  "baseBranches": [
    "master"
  ],
  "prHourlyLimit": 2
}
